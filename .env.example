TZ=Asia/Shanghai

# ===========================
# MySQL 配置（容器内部）
# ===========================
MYSQL_ROOT_PASSWORD=
MYSQL_DATABASE=ruoyi
MYSQL_USER=root
MYSQL_PASSWORD=

# ===========================
# Redis 配置（默认不启用密码）
# ===========================
REDIS_PASSWORD=

# ===========================
# 阿里云 OSS（可选）
# - 未配置时可以先禁用或仅在本地上传到默认磁盘
# - 部署到正式环境时务必改为自己的密钥与 Bucket
# ===========================
ALIYUN_OSS_ENABLED=true
ALIYUN_OSS_ENDPOINT=https://oss-cn-shanghai.aliyuncs.com
ALIYUN_OSS_ACCESS_KEY_ID=
ALIYUN_OSS_ACCESS_KEY_SECRET=
ALIYUN_OSS_BUCKET=
ALIYUN_OSS_CUSTOM_DOMAIN=
ALIYUN_OSS_BASE_PATH=uploads

# ===========================
# 智谱 AI（问卷 AI 汇总）
# ===========================
ZAI_API_KEY=

# ===========================
# 镜像仓库配置
# ===========================
DOCKER_REGISTRY=docker.io
IMAGE_NAMESPACE=your-docker-username  # 修改为 Docker Hub 用户名
